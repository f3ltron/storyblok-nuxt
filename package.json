{
  "name": "storyblok-nuxt",
  "version": "0.0.0-development",
  "description": "Storyblok Nuxt.js module",
  "license": "MIT",
  "contributors": [
    {
      "name": "Alexander Feiglstorfer <delooks@gmail.com>"
    },
    {
      "name": "Florent Giraud <hello@florent.dev>"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/storyblok/storyblok-nuxt",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "npm run test:e2e",
    "test:e2e": "npm run cypress:run",
    "test:e2e:dev": "npm run cypress:dev",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "start": "nuxt start",
    "run": "cypress run",
    "open": "cypress open",
    "cypress:run": "npm run build && start-server-and-test start http://localhost:3000 run",
    "cypress:dev": "start-server-and-test dev http://localhost:3000 open"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "storyblok-js-client": "^3.3.1",
    "storyblok-vue": "^1.0.5"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "chai": "^4.2.0",
    "cypress": "^6.2.1",
    "nuxt": "^2.14.12",
    "start-server-and-test": "^1.11.7"
  }
}
